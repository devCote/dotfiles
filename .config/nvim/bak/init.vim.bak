:set number
:set ts=4
:set sw=4

call plug#begin('~/.vim/plugged')

Plug 'chemzqm/vim-jsx-improve'
Plug 'leafgarland/typescript-vim'
Plug 'ianks/vim-tsx'
Plug 'neoclide/coc.nvim', {'branch': 'master', 'do': 'yarn install --frozen-lockfile'}
Plug 'preservim/nerdtree'

let g:coc_global_extensions = ['coc-json', 'coc-git', 'coc-tsserver', 'coc-emmet', 'coc-css', 'coc-eslint', 'coc-graphql', 'coc-html', 'coc-python', 'coc-sh', 'coc-sql', 'coc-snippets', 'coc-tslint', 'coc-prettier'] 

let g:jsx_improve_javascriptreact = 0 

call plug#end()


